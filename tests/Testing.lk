fonction sum(a: entier, b: entier): entier
                {
                    retourner a - b
                }
ecrire("entrer votre année de naissance:")
var x = entier(lire())
ecrire("en quelle année sommes nous ??")
var y = entier(lire())
var z = sum(y, x)
ecrire("votre age est: "+ chaine(z))
si z < 15
    ecrire("tu es un enfant")
sinon si z >= 15 && z < 60
    ecrire("tu es jeune")
sinon
    ecrire("tu es vieux")
